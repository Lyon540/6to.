Practica 4

Create Database Empresa
Use Empresa

Create Table Proveedor(
Codigo int Primary Key,
Nombre varchar (60),
Direccion varchar (60),
Ciudad varchar (30),
Correo varchar (40) default 'Sin Correo',
Telefono varchar (20) default 'Sin telefono'
)

