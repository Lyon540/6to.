/*Ejercicio 6*/
select * from Hospitales
where Direccion= ' '

/*Ejercicio 7*/
select * from Hospitales
where Telefono is null

/*Ejercicio 8*/
select * from Hospitales
where Telefono= ' '

/*Ejercicio 9*/
