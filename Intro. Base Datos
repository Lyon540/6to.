Create database UMM
Use UMM

Create Table Alumnos(
Matricula Int Primary Key,
Nombre Varchar(30),
Apellido varchar (50),
Localidad varchar (50),
Fecha_Nac datetime,
Direccion varchar (50),
Curso Int,
Nivel varchar(30),
Faltas int
)

Insert into Alumnos values ('123456','Juan Miguel','Soler Vidal','Monterrey', '1985-10-10', 'Mar Jonico 214', '1', 'Profesional', '15');
