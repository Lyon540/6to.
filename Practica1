Problema 6
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication4
{
    class Program
    {
        static void Main(string[] args)
        {
                int[] lista1 = new int[] {70,60,50,80,100};
                int suma1 = 0;
                int promedio1 = 0;
            int[] lista2 = new int[] { 90, 80, 40, 70, 60 };
            int suma2 = 0;
            int promedio2 = 0;
                Console.WriteLine("Las calificaciones de la materia Fisica son:");
                for (int i = 0; i < 5; i++)
                {
                    suma1 += lista1[i];
                    Console.WriteLine(lista1[i]);
                    promedio1 = suma1 / 5;
                }

            Console.WriteLine("Las calificaciones de la materia Matematicas son:");
                for (int j=0;j<5;j++)
            {
                suma2 += lista2[j];
                Console.WriteLine(lista2[j]);
                promedio2 = suma2 / 5;
            }
                Console.WriteLine("El total de la suma de la materia Fisica es:");
                Console.WriteLine(suma1);
                Console.WriteLine("El promedio del grupo A es:");
                Console.WriteLine(promedio1);

            Console.WriteLine("El total de la suma de la materia Matematicas es:");
            Console.WriteLine(suma2);
            Console.WriteLine("El promedio del grupo B es:");
            Console.WriteLine(promedio2);
            Console.ReadKey();
            }
        }
    }

Problema 7

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication5
{
    class Program
    {
        static void Main(string[] args)
        {
            float[] EstaturaHombres = new float[] {177,168,150,180,190};
            float SumaHombres = 0;
            float PromedioHombres = 0;

            float[] EstaturaMujeres = new float[] { 164,170,150,140,157 };
            float SumaMujeres = 0;
            float PromedioMujeres = 0;

            Console.WriteLine("La estatura de los Hombres son:");
            for (int i =0; i<5; i++)
            {
                Console.WriteLine(EstaturaHombres[i]);
                SumaHombres += EstaturaHombres[i];
                PromedioHombres = SumaHombres / 5;
            }
            Console.WriteLine("Las estaturas promedio de los hombres son:");
            Console.WriteLine(PromedioHombres);
            
           

            /*Comienza lo de las mujeres*/
            Console.WriteLine("La estatura de las Mujeres son:");
            for (int j = 0; j < 5; j++)
            {
                Console.WriteLine(EstaturaMujeres[j]);
                SumaMujeres += EstaturaMujeres[j];
                PromedioMujeres = SumaMujeres / 5;
            }

            Console.WriteLine("Las estaturas promedio de las Mujeres son:");
            Console.WriteLine(PromedioMujeres);
            Console.ReadKey();
        }
    }
}

