using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Salario");
            int Salario = int.Parse(Console.ReadLine());
            int x = (Salario - 500)/2;
            int y = (x + 500);
            Console.WriteLine("Salario1={0} Salario2={1}", x, y);
            Console.ReadLine();
        }
    }
}


Segundo Parcial

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 0, total = 0;
            for(int i=0;i<12;i++)
              {
                Console.WriteLine("Teclea el Ahorro");
                a = int.Parse(Console.ReadLine());
                total += a;
                Console.WriteLine("total ahorrado={0}", total);
            }
        }
    }

}

Programa de examen 1

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication2
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] meses = new int[12];
            int suma = 0;
            for (int i = 0; i < 12; i++)
            {
                Console.WriteLine("Teclea el ahorro");
                meses[i] = int.Parse(Console.ReadLine());
                suma += meses[i];
            }
            for (int i = 0; i < 12; i++)
            {
                Console.WriteLine(meses[i]);
            }
            Console.WriteLine(suma);
            Console.WriteLine(meses.Average());
            Console.ReadKey();
        }
    }
}

Problema de clase

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication3
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] lista = new int[] { 7, 8, 6, 5, 9 };
            int suma = 0;
            for (int i = 0; i < 5; i++)
            {
                suma += lista[i];
            }
            Console.WriteLine(suma);
            for (int i = 0; i < 5; i++)
            {
                Console.WriteLine(System.Math.Pow(lista[i], 2));
                Console.WriteLine(System.Math.Pow(lista[i], 3));
            }
            Double sumatoria = 0;
            Double promedio = lista.Average();
            for (int i = 0; i < 5; i++)
            {
                sumatoria += System.Math.Pow(promedio - lista[i], 2);
            }
            Double varianza = sumatoria / 5;
            Double desviacion = System.Math.Sqrt(varianza);
            Console.WriteLine(desviacion);
            Console.ReadKey();
        }
    }
}


2do

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication2
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] lista = new string[5];
            for (int i=0;i < 5; i++)
            {
                Console.WriteLine("Teclea un Caracter");
                lista[i] = Console.ReadLine();
            }
            for (int i=0; i < 5; i++)
            {
                Console.WriteLine(lista[i]);
            }
            Console.ReadKey();
        }
    }
}

Metodo de burbuja

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] lista = new int[] { 20, 50, 90, 80, 70 };
            int aux = 0;
            for (int i=0;i<5;i++)
            {
                for(int j=i+1;j<5;j++)
                {
                    if (lista[i] < lista[j])
                    {
                        aux = lista[j];
                        lista[j] = lista[i];
                        lista[i] = aux;
                    }
                }
            }
            for(int i=0; i<5; i++)
            {
                Console.WriteLine(lista[i]);
            }
            Console.ReadKey();
        }

    }
}

